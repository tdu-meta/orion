# Data Provider Configuration
DATA_PROVIDER__PROVIDER=alpha_vantage
DATA_PROVIDER__API_KEY=your_alpha_vantage_api_key_here
DATA_PROVIDER__RATE_LIMIT=5

# Cache Configuration
CACHE__QUOTE_TTL=300
CACHE__OPTION_CHAIN_TTL=900
CACHE__HISTORICAL_TTL=86400

# Notification Configuration
NOTIFICATIONS__EMAIL_ENABLED=true
NOTIFICATIONS__SMTP_HOST=smtp.gmail.com
NOTIFICATIONS__SMTP_PORT=587
NOTIFICATIONS__SMTP_USER=your_email@gmail.com
NOTIFICATIONS__SMTP_PASSWORD=your_app_password
NOTIFICATIONS__FROM_ADDRESS=your_email@gmail.com
NOTIFICATIONS__TO_ADDRESSES=["recipient@example.com"]

# Screening Configuration
SCREENING__DEFAULT_STOCK_UNIVERSE=["SPY_500"]
SCREENING__MAX_CONCURRENT_REQUESTS=5

# Logging Configuration
LOGGING__LEVEL=INFO
LOGGING__FORMAT=json
LOGGING__OUTPUT=stdout
